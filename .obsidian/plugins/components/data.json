      "maxWidthRatio": -1,
      "query": "      // Êó•ÊúüÊ†ºÂºèÂåñ\n      const dateFormat = \"YYYY-MM-DD\"\n      // Ë°®Â§¥ÔºöÊúüÊúõÁöÑÂ±ïÁ§∫ÁöÑÂ±ûÊÄßÂàóË°®\n      const headers = [\"status\"]\n      // ÂΩìÂâçÈ°µÁ†Å\n      const pageNum = {{pageNum}};\n      // ÂçïÈ°µÊï∞Èáè\n      const pageSize = {{pageSize}};\n      // Êñá‰ª∂ÂêçÁß∞\n      const pageTitleLike = \"{{pageTitle}}\"\n\n      // Ê†áÁ≠æ\n      const tagLike = \"{{tag}}\"\n      const allTags={{alltags}}\n      const tagMatch = (page, tag) => {\n        if (tag) {\n         return page.file.tags && page.file.tags.some(t => t.includes(tagLike));\n        }\n        return true;\n      }\nconst tagis = (page, tag,alltags) => {\n       if(alltags) return page.file.tags;\n        else if (page.file.tags && page.file.tags.some(t => t.includes(tagLike))) {\n         return tag;\n        }\n        return page.file.tags;\n      }\n\n\n      const data = dv.pages(\"\")\n      .where(p => p.file.name?.toLowerCase().includes(pageTitleLike?.toLowerCase()))\n      .where(p => tagMatch(p, tagLike))\n      .sort(p => p.file.mtime, \"{{sort}}\")\n      .map(p => {\n        return [p.file.link, ...headers.map(property => p[property ]),tagis(p, tagLike,allTags), moment(Number(p.file.mtime)).format(dateFormat)];\n      })\n\n\n\n\n      const pageData = data.slice((pageNum - 1) * pageSize, pageNum * pageSize)\n        .limit(pageSize)\n      dv.paragraph(\"ÂÖ±Êúâ \" + data.length +\" Êù°Êï∞ÊçÆ\")\n      dv.table([\"name\", ...headers,\"tags\", \"mtime\"], pageData)\n",
